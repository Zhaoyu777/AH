{% extends 'bootstrap-modal-layout.html.twig' %}

{% set modal_class= "modal-lg sign-modal" %}

{% block title %}
  活动详情
{% endblock %}

{% block body %}
  <iframe id="task-content-iframe" class="task-content-iframe"
          src="{{ path('course_task_activity_show',{courseId:courseId, id:taskId, mode:'report'}) }}"
          style="width:100%;height:500px;border:0px"
          allowfullscreen webkitallowfullscreen>
  </iframe>
{% endblock %}

{% block footer %}
  <a class="btn btn-primary" data-dismiss="modal" href="">关闭</a>
{% endblock %}
